<p id="notice"><%= notice %></p>

<h1>Favicons</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Image type</th>
      <th>Site</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @favicons.each do |favicon| %>
      <tr>
        <td><%= favicon.image %></td>
        <td><%= favicon.image_type %></td>
        <td><%= favicon.site_id %></td>
        <td><%= link_to 'Show', favicon %></td>
        <td><%= link_to 'Edit', edit_favicon_path(favicon) %></td>
        <td><%= link_to 'Destroy', favicon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Favicon', new_favicon_path %>
